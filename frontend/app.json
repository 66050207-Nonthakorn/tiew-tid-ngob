{
  "expo": {
    "name": "เที่ยวติดงบ",
    "slug": "tiew-tid-ngob",
    "version": "1.0.0",
    "scheme": "tiew-tid-ngob",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "dark": {
            "image": "./assets/icon.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/NunitoSans-Regular.ttf",
            "./assets/fonts/NunitoSans-Light.ttf",
            "./assets/fonts/NunitoSans-Bold.ttf",
            "./assets/fonts/NunitoSans-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyAnzRwGVZoN1WwBJkr53Og2LA2Dj6M3fjs"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.applentk.tiew-tid-ngob"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAnzRwGVZoN1WwBJkr53Og2LA2Dj6M3fjs"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.applentk.tiew_tid_ngob",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "79369ee5-56b3-4104-9d02-de109cfde12c"
      }
    }
  }
}
